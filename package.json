{
  "name": "@selas/selas-node",
  "version": "0.1.3",
  "description": "Client for the Selas API using NodeJS",
  "packageManager": "pnpm@7.13.2",
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "commonjs",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "author": "Benjamin Trom",
  "license": "MIT",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag && pnpm publish --no-git-checks",
    "start": "node --loader=ts-node/esm src/index.ts",
    "test": "jest --setupFiles dotenv/config",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@supabase/supabase-js": "^2.1.0",
    "pusher-client": "1.1.0"
  },
  "devDependencies": {
    "jest": "^29.3.1",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.2"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "openapi-types",
        "vite",
        "@babel/core"
      ]
    }
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.1.0",
    "dotenv": "^16.0.3",
    "pusher-client": "^1.1.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SelasStudio/selas-js.git"
  },
  "keywords": [
    "API",
    "Client",
    "for",
    "Selas",
    "API",
    "(https://selas.ai)"
  ],
  "bugs": {
    "url": "https://github.com/SelasStudio/selas-js/issues"
  },
  "homepage": "https://github.com/SelasStudio/selas-js#readme"
}
